{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 15
    },
    "rules": {
        "no-console":"off",
        "import/no-extraneous-dependencies":"off",
        "no-unused-vars":"off",
        "linebreak-style":"off",
        "indent":"off",
        "eol-last":"off"
    }
}
